{
  "name": "intuition",
  "displayName": "Intuition",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "THP",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@0xintuition/1ui": "^0.3.6",
    "@0xintuition/graphql": "^0.6.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@tanstack/react-query": "^5.69.0",

    "graphql-request": "^7.1.2",

    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.483.0",

    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "prettier": "3.2.4",
    "svgo": "^3.0.2",
    "tailwindcss": "3.3.0",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "node_modules/@0xintuition/1ui/dist/*"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ],
    "name": "Intuition"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}
